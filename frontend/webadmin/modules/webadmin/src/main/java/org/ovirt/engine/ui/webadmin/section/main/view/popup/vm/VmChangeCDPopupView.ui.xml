<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:d="urn:import:org.ovirt.engine.ui.webadmin.widget.dialog" xmlns:e="urn:import:org.ovirt.engine.ui.webadmin.widget.editor">

	<ui:style type="org.ovirt.engine.ui.webadmin.section.main.view.popup.vm.VmChangeCDPopupView.Style">
		.content {
		}		
		
		.isoImageEditor {
			width: 100%;
		}
		
		.isoImageEditorLabel {
			display: none;
		}
		
		.isoImageEditorBox {
			width: 100%;
			padding: 0px;
		}
		
	</ui:style>
	
	<d:SimpleDialogPanel width="400px" height="170px">
		<d:content>
			<g:VerticalPanel verticalAlignment="ALIGN_MIDDLE" addStyleNames="{style.content}">
				<e:ListModelListBoxEditor ui:field="isoImageEditor" addStyleNames="{style.isoImageEditor}" />
			</g:VerticalPanel>
		</d:content>
	</d:SimpleDialogPanel>

</ui:UiBinder>
